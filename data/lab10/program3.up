program Program3;

    var A : integer;
        B : float;
        C : integer;

    begin
        Write('Enter an integer for A: ');
        Read(A);
        Writeln('');
        Write('Enter a float for B: ');
        Read(B);
        Writeln('');
        Write('Enter an integer for C: ');
        Read(B);
        Writeln('');

        Read(A, B, C);
        A := 8*A*A*A + 5*B*B - C;
        Write('A = ', A, ', A = ', B,', and A = ', C);
    end.